{
  "expo": {
    "name": "software_incidents_mairie",
    "slug": "software_incidents_mairie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "testios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["NOTIFICATIONS", "SYSTEM_ALERT_WINDOW"],
      "package": "com.kubarthur.software_incidents_mairie"
    },
    "extra": {
      "expo": {
        "android": {
          "useNextNotificationsApi": true
        }
      },
      "eas": {
        "projectId": "c3899762-9110-4804-833b-7e8692e0eeed"
      }
    },
    "plugins": [
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsVersion": "11.0.15"
        }
      ]
    ]
  }
}